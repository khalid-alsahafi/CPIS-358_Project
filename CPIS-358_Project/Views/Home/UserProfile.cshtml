@model CPIS_358_Project.Models.User
@{
	ViewData["Title"] = "Profile";
}
@if (User.Identity?.IsAuthenticated == true)
{
	<h1>Welcome @User.Identity.Name, this is your profile page.</h1>
	<p>Highest score: @Model.HighestScore</p>
	<p>Latest score: @Model.LastScore</p>
}
else
{
	<h1>You are NOT logged in</h1>
}